{
    "manifest_version": 3,
    "name": "LinkedIn Job Scraper",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": [
                "src/content.ts"
            ],
            "matches": [
                "https://www.linkedin.com/jobs/*"
            ]

        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}